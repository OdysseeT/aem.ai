<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Page">
    <jcr:content
        cq:contextHubPath="/etc/cloudsettings/default/contexthub"
        cq:contextHubSegmentsPath="/etc/segmentation/contexthub"
        cq:template="/conf/aemai/settings/wcm/templates/page-content"
        jcr:primaryType="cq:PageContent"
        jcr:title="login"
        sling:resourceType="aemai/components/page"
        pageTitle="AEM.AI - Login">
        <root
            jcr:primaryType="nt:unstructured"
            sling:resourceType="aemai/components/container"
            layout="responsiveGrid">
            <container
                jcr:primaryType="nt:unstructured"
                sling:resourceType="aemai/components/container">
                <title
                    jcr:primaryType="nt:unstructured"
                    jcr:title="Authentication failed!"
                    sling:resourceType="aemai/components/title"/>
                <container
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="aemai/components/container"
                    layout="responsiveGrid">
                    <text
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="aemai/components/text"
                        text="Your account has been locked for 5 minutes"
                        textIsRich="true"/>
                </container>
            </container>
        </root>
    </jcr:content>
</jcr:root>
